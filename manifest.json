{
	"browser_specific_settings": {
		"gecko": {
			"strict_min_version": "61"
		}
	},
	"background": {
		"page": "background.html"
	},
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": ["folderBar.js"]
		}
	],
	"browser_action": {
		"browser_style": true,
		"default_title": "Group Tabs",
		"default_popup": "tabs.html"
	},
	"description": "A Folder Tab creator & manager.",
	"manifest_version": 2,
	"name": "Group Tabs",
	"permissions": ["tabs", "storage", "tabHide", "contextMenus"],
	"version": "1.0"
}
